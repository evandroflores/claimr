{
    "Aggregate": true,
    "Concurrency": 1,
    "Cyclo": 5,
    "Deadline": "2m",
    "DisableAll": true,
    "Exclude": ["vendor/.*", ".*_test", ".*/.*_test"],
    "Enable": [
        "deadcode",
        "dupl",
        "errcheck",
        "gas",
        "goconst",
        "gocyclo",
        "gofmt",
        "goimports",
        "golint",
        "gotype",
        "gotypex",
        "ineffassign",
        "interfacer",
        "lll",
        "maligned",
        "megacheck",
        "misspell",
        "nakedret",
        "safesql",
        "structcheck",
        "test",
        "testify",
        "unconvert",
        "unparam",
        "varcheck",
        "vet",
        "vetshadow"
    ],
    "EnableGC": true,
    "LineLength": 140,
    "Test": true,
    "Vendor": true,
    "VendoredLinters": true
}
